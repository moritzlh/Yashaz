Pr√§senzaufgaben

#include <stdio.h>
#include <stdlib.h>
#include <stdint.h>


 void Aufgabe1(){
    int i = 1;
    while(i < 100){
        if(i % 3 == 0){
            printf("%d\n",i);
        }
        i++;
    }
 }

  void Aufgabe2(){

    int f = 1 , n = 7, i = 0;
    do {
        i++;
        f = f * i;
    }
    while(i < n);
    printf("%d",f);
  }








  void Aufgabe4() {

 long int n = 0 , b , a = 1;
    while( n <= 12345678){
        n = 10 * n + a;
        b = n * 8 + 9;
        a++;
        printf("%li\n",b);


    }


}

 void Aufgabe5() {

   long int n = 0 , b , a = 1;
    while( n <= 12345678){
        n = 10 * n + a;
        a++;
        b = n * 9 + a;
        printf("%li\n",b);










    }
}

 void Aufgabe6() {
 long int n = 0 , b , a = 9;
    while( n <= 9876543){
        n = 10 * n + a;
        a--;
        b = n * 9 + a - 1;
        printf("%li\n",b);

    }
 }




