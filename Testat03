#include <stdio.h>
#include <stdlib.h>
#include <stdint.h>

void Maschinengenauigkeit(){
    float a = 1;
    int count = 0;
    while( a + 1 > 1){

        a = a / 2;
        count++;

    }
    printf("%d\n", count );




}
void Eule(){
    double c = 1 , E = 2.71828182845904523;
    long int f = 1;
    int i = 0;
    while ( c < E ){
        i++;
        f = (i * f);
        c = c + (1.0 / f);
        printf("%d | %.15f\n",  i,c);
    }

}

void ggt(){
    uint16_t a = 210, b = 80, c;
    while (a != 0 && b != 0){
         c = abs(a - b);
            if( a > b ){
        a = c;
        }
    else {
            b = c;
    }
    }
    if( a > b){
    printf("%u" , a);
    }
    else { printf("%u", b);
    }
}

void ggt2neu(){

    int a = 80, b = 210, c;
    while(b != 0){
    c = abs(a - b);
    if( a < b ){
        c = abs(a - b);
        a = b;
        b = c;
       }
    else {
           c = a - b;
    while( c >= b){
        c = c - b;


    }
    a = b;
    b = c;
    }
    }
    printf("%d",a);
}



void modulo(){

    uint16_t a = 5, b = 2;
    int c;
    c = a - b;
    if( a < b){
        printf("%d", a);
       } else {
    while( c >= b){
        c = c - b;


    }
    printf("%d",c);
    }

    }



void ggt2alt(){
    int a = 80, b = 210, c;
    while(b != 0){
        c = a%b;
        a = b;
        b = c;


    }
    printf("%d", a);




}

void ggt2neuTEST(){

    int a = 80, b = 210, c;
    while(b != 0){
    if( a < b ){
        c = a;
       }
    else {
           c = a - b;
    while( c >= b){
        c = c - b;

    }
    }
    a = b;
    b = c;
    }

    printf("%d",a);
}













int main()
{
    Aufgabe2();
    return 0;
}
